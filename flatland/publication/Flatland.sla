            <ITEXT CH="      18-19 ô ligne ! ô carré! ô cercle! ô cube! ô sphère! Que les"/>
            <para/>
            <ITEXT CH="    mystères des entrailles de flatland sont vraiment secrets! à quel"/>
            <para/>
            <ITEXT CH="     regard fusse-je exposé? Quel guide divin reste visible lorsque"/>
            <para/>
            <ITEXT CH="                        les figures se trompent?"/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
