            <ITEXT CH="     13-14 Un étranger capable de distinguer la mort, parut dans le"/>
            <para/>
            <ITEXT CH="     royaume universel de l'ignorance. étant conscient de la coutume"/>
            <para/>
            <ITEXT CH="    du pays, il coupa un point  du corps de la femme du monarque. Une"/>
            <para/>
            <ITEXT CH="     personne s'écria que les bouches de l'altesse étaient comme des"/>
            <para/>
            <ITEXT CH="                             yeux sans vie."/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
            <para/>
