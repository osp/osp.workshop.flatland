            <ITEXT CH="1. La concision est merveilleuse."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="2. Écrivez des programmes qui font une seule chose mais qui"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="   le font bien."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="3. Concevez un prototype dès que possible."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="4. Préférez la portabilité à lʼefficacité."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="5. Stockez les données en ASCII."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="6. Utilisez le levier du logiciel à votre avantage."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="7. Utilisez les scripts shell pour améliorer lʼeffet de"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="   levier et la portabilité."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="8. Évitez les interfaces utilisateur captives."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="9. Faites de chaque programme un filtre."/>
            <para PARENT="Default Paragraph Style"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="_La Philosophie Unix_, Mike Gancarz, 1994"/>
            <para PARENT="Default Paragraph Style"/>
            <para PARENT="Default Paragraph Style"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Une des qualités du logiciel libre est dʼencourager la création"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="de ses propres outils en piochant dans un écosystème de"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="programmes existants. Si cela a été socialisé par la création"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="dʼun cadre juridique"/>
            <nbspace/>
            <ITEXT CH="propice, on retrouve certains des"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="fondements"/>
            <nbspace/>
            <ITEXT CH="techniques dans le système dʼexploitation UNIX,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="sur"/>
            <nbspace/>
            <ITEXT CH="lequel est basé GNU Linux et Mac OS, dont un des"/>
            <nbspace/>
            <ITEXT CH="concepts"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="les plus brillants est celui de"/>
            <nbspace/>
            <ITEXT CH="_pipeline_. Il y existe des"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="milliers de logiciels"/>
            <nbspace/>
            <ITEXT CH="en ligne de commande (interface textuelle"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="pour interagir avec un ordinateur) qui, comme le stipule le"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="point numéro 2, ne font quʼune seule et simple"/>
            <nbspace/>
            <ITEXT CH="chose. Mais la"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="_magie_ réside dans la possibilité"/>
            <nbspace/>
            <ITEXT CH="de combiner ces différents"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="programmes ou «briques"/>
            <nbspace/>
            <ITEXT CH="logicielles»: le résultat de lʼune peut"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="alors en"/>
            <nbspace/>
            <ITEXT CH="alimenter une autre, et ainsi de suite. Il devient dès"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="lors très facile de se créer son propre logiciel en combinant"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="ces briques. Plutôt que"/>
            <nbspace/>
            <ITEXT CH="dʼêtre un couteau suisse voulant"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="répondre à tous les besoins, UNIX est une boîte à outils."/>
            <para PARENT="Default Paragraph Style"/>
            <para PARENT="Default Paragraph Style"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Un seul caractère typographique, une seule taille de corps, pas"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="de gras, pas dʼitalique, pas de souligné… Cela semble ne pas"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="laisser beaucoup de libertés de mise en page, et pourtant!  Nous"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="découvrirons les joies du texte brut (texte sans enrichissement"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="visuel) et de lʼASCII Art (images composées uniquement de"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="glyphes) en produisant collectivement et à partir de filtres"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="textuels simples des objets éditoriaux."/>
